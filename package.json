{
  "name": "tailwind-typescript-starter",
  "version": "1.0.0",
  "description": "A starter project using Tailwind CSS, TypeScript, and Flowbite to help you get started with your applications.",
  "main": "index.js",
  "scripts": {
    "setup": "npx tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "test": "npx jest",
    "lint": "eslint --no-ignore src --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,css,scss,md}'",
    "watch": "npx webpack --watch",
    "start": "live-server --open=./index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/themesberg/tailwind-typescript-starter.git"
  },
  "keywords": [
    "typescript",
    "tailwind",
    "tailwind",
    "css",
    "flowbite"
  ],
  "author": "Bergside Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/themesberg/tailwind-typescript-starter/issues"
  },
  "homepage": "https://github.com/themesberg/tailwind-typescript-starter#readme",
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.15.0",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "jest": "^29.7.0",
    "live-server": "^1.2.2",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.2.4",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^5.1.0"
  },
  "dependencies": {
    "flowbite": "^2.4.1"
  }
}
